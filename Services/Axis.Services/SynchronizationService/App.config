    <add key="Debug" value="true" />
    <!--DebugLogFile shoule be used in production environment which should be monitored while in use-->
    <add key="DebugLogFile" value="false" />
    <add key="LDAPConnection" value="" />
    <add key="LDAPPath" value="LDAP://xenatix-qa.cloudapp.net/CN=Users,DC=xenatix,DC=local" />
    <add key="ADServerName" value="xenatix-qa.cloudapp.net" />
    <add key="LDAPPort" value="389" />
