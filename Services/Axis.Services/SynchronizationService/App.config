  <connectionStrings>
    <add name="XenatixDBConnection" connectionString="Server=.;Initial Catalog=Axis;Integrated Security=SSPI;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="ScheduleInterval" value="1" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
