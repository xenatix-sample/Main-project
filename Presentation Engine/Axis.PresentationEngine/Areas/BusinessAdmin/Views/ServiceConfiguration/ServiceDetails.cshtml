<div class="row padding-right-small margin-bottom-3x padding-left-small">
    <div class="panel panel-light padding-xsmall">
        <div class="panel-body">
            <form role="form" id="serviceDetailsForm" name="serviceDetailsForm" xen-check-form is-default="true" data-on-save="save(isNext, isMandatory, hasErrors)" bs-modifiable="true" set-form-status novalidate set-form-mode permission="{{resolvePermission(pageSecurity)}}" permission-key="{{permissionKey}}">
                <div security permission-key="{{permissionKey}}" permission="{{resolvePermission(pageSecurity)}}" permission-mode="readonly">
                    <ng-form name="serviceDetailsForm" novalidate bs-modifiable="true" set-form-status="child">
                        <div class="row padding-bottom-xsmall padding-top-xsmall header-compact show">
                            <div class="col-lg-3 col-md-3 col-sm-6 margin-left padding-top-xsmall">
                                <dl>
                                    <dt>Service Code</dt>
                                    <dd>{{serviceDefinition.ServiceCode}}</dd>
                                </dl>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-6 padding-top-xsmall">
                                <dl>
                                    <dt>Service Name</dt>
                                    <dd>{{serviceDefinition.ServiceName}}</dd>
                                </dl>
                            </div>
                            <div class="col-lg-3 col-lg-offset-1 col-md-3 col-sm-6 margin-right-2x padding-top-xsmall">
                                <dl>
                                    <dt>Service Type</dt>
                                    <dd>{{serviceDefinition.ServiceType}}</dd>
                                </dl>
                            </div>
                        </div>
                        <div>&nbsp;&nbsp;</div>
                        <div class="panel-body margin-left-small nopadding-top">
                            <div class="row">
                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 padding-bottom-small">
                                    <div class="form-group-lg padding-bottom-small" data-ng-class="{ 'has-error' : serviceDetailsForm.ServiceWorkflow.$invalid && !serviceDetailsForm.ServiceWorkflow.$pristine}">
                                        <label for="ServiceWorkflow">Service Workflow <sup>*</sup></label>
                                        <select ng-required="true" name="ServiceWorkflow" data-validation-name="Service Workflow"  data-ng-model="serviceDetails.ModuleComponentID" ng-disabled="isServiceWorkflowDisabled()" id="ServiceWorkflow" class="form-control" data-ng-options="serviceWorkflow.ID as serviceWorkflow.Name for serviceWorkflow in serviceWorkflowData">
                                            <option value="">Select</option>
                                        </select>
                                        <span class="select-btn"><i class="xen-select"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="panel-body nopadding-top border-top">
                            <div class="panel-body panel-scroll ps-active-y">
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 padding-bottom">
                                        <xen-multi-select-typeahead class="xen-multi-select" label="Delivery Method" data-validation-name="Delivery Method" form-name="serviceDetailsForm" show-selected="true" element-id="deliveryMethod" element-name="deliveryMethod" selected-values="serviceDetails.DeliveryMethods" ng-model="deliveryMethods" target-text-key="DeliveryMethod" target-value-key="DeliveryMethodID" source-text-key="Name" source-value-key="ID"></xen-multi-select-typeahead>
                                    </div>
                                    <div class="col-lg-4 col-lg-offset-3 col-md-4 col-sm-4 col-xs-4 padding-bottom">
                                        <xen-multi-select-typeahead class="xen-multi-select" label="Place Of Service" data-validation-name="Place Of Service" form-name="serviceDetailsForm" show-selected="true" element-id="placeOfService" element-name="placeOfService" selected-values="serviceDetails.ServiceLocation" ng-model="serviceLocation" target-text-key="ServiceLocation" target-value-key="ServiceLocationID" source-text-key="Name" source-value-key="ID"></xen-multi-select-typeahead>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 padding-bottom">
                                        <xen-multi-select-typeahead class="xen-multi-select" label="Recipient" data-validation-name="Recipient" form-name="serviceDetailsForm" show-selected="true" element-id="recipient" element-name="recipient" selected-values="serviceDetails.Recipients" ng-model="recipients" target-text-key="CodeDescription" target-value-key="CodeID" source-text-key="Name" source-value-key="ID"></xen-multi-select-typeahead>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 padding-bottom">
                                        <xen-multi-select-typeahead class="xen-multi-select" label="Service Status" data-validation-name="Service Status" form-name="serviceDetailsForm" show-selected="true" element-id="serviceStatus" element-name="serviceStatus" selected-values="serviceDetails.ServiceStatus" ng-model="serviceStatus" target-text-key="ServiceStatus" target-value-key="ServiceStatusID" source-text-key="Name" source-value-key="ID"></xen-multi-select-typeahead>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-lg-offset-3 col-sm-4 col-xs-4 padding-bottom">
                                        <xen-multi-select-typeahead class="xen-multi-select" label="Attendance Status" data-validation-name="Attendance Status" form-name="serviceDetailsForm" show-selected="true" element-id="attendanceStatus" element-name="attendanceStatus" selected-values="serviceDetails.AttendanceStatus" ng-model="attendanceStatus" target-text-key="AttendanceStatus" target-value-key="AttendanceStatusID" source-text-key="Name" source-value-key="ID"></xen-multi-select-typeahead>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 padding-bottom">
                                        <xen-multi-select-typeahead class="xen-multi-select" label="Tracking Field" data-validation-name="Tracking Field" form-name="serviceDetailsForm" show-selected="true" element-id="trackingField" element-name="trackingField" selected-values="serviceDetails.TrackingField" ng-model="trackingField" target-text-key="TrackingField" target-value-key="TrackingFieldID" source-text-key="Name" source-value-key="ID"></xen-multi-select-typeahead>
                                    </div>
                                    <div class="col-lg-4 col-md-4 col-lg-offset-3 col-sm-4 col-xs-4 padding-bottom">
                                        <xen-multi-select-typeahead class="xen-multi-select" label="Credentials" data-validation-name="Credentials" form-name="serviceDetailsForm" show-selected="true" element-id="credentials" element-name="credentials" data-validation-name="Credentials" selected-values="serviceDetails.Credentials" ng-model="credentials" target-text-key="CredentialName" target-value-key="CredentialID" source-text-key="Name" source-value-key="ID"></xen-multi-select-typeahead>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </ng-form>
                </div>
                <div class="row padding-top-small">
                    <div class="col-lg-6 col-md-6">
                        <p class="uppercase subtext"><sup>*</sup> Indicates a required field</p>
                    </div>
                    <div class="col-lg-6 col-md-6 text-right nopadding-left">
                        <xen-submit id="btnSave" name="btnSave" value="SAVE" is-next="false" mandatory="true" style="btn btn-default btn-lg"></xen-submit>
                    </div>
                </div>
                <div>&nbsp;&nbsp;</div>

                <h3 class="uppercase nomargin padding-xsmall border-top">Service Workflows</h3>
                <div class="add_me">
                    <plus-button tabindex="0" title="New Program Unit" reset="initServiceDetails()" permission-key="{{permissionKey}}" permission="create"></plus-button>
                </div>
                <div class="row padding-small">
                    <table id="serviceWorkflowTable" data-toggle="table"
                           data-sort-order="desc"
                           data-show-columns="true"
                           data-search="true"
                           data-minimum-count-columns="2"
                           data-filter-control="false"
                           data-mobile-responsive="true"
                           data-filter-bar
                           data-advanced-search="false"
                           data-show-multi-sort="true"
                           data-classes="table table-stripe-borders"
                           bootstrap-table="tableoptions" grid-arrow-selector>
                        <thead>
                        </thead>
                    </table>

                </div>
            </form>
        </div>
    </div>
</div>
