<div class="row padding-right-small margin-bottom-3x padding-left-small">
    <div class="panel panel-light padding-xsmall">
        <div class="panel-body">

            <form role="form" id="planAddressForm" name="planAddressForm" xen-check-form is-default="true" data-on-save="save(isNext, isMandatory, hasErrors)"  bs-modifiable="true" set-form-status="planAddressForm" set-form-mode permission="{{resolvePermission(pageSecurity)}}" permission-key="{{permissionKey}}">
                <div security permission-key="{{permissionKey}}" permission="{{resolvePermission(pageSecurity)}}" is-disabled="{{isAddressDisabled}}" permission-mode="readonly">
                    <h3 class="uppercase nomargin padding-xsmall border-top">PLAN ADDRESS</h3>
                    <div class="row padding-top-small">
                        <div class="section-block col-md-12 padding-bottom-small">
                            <div class="col-md-10">
                                <div class="col-md-3 padding-bottom-small">
                                    <div class="form-group-lg padding-bottom-small" ng-class="{ 'has-error' : planAddressForm.ElectronicPayorID.$invalid && !planAddressForm.ElectronicPayorID.$pristine }">
                                        <label for="payorId">Electronic Payor ID </label>
                                        <input type="text" ng-model="planAddress.ElectronicPayorID" data-ng-maxlength="20" id="ElectronicPayorID" name="ElectronicPayorID" class="form-control" auto-focus event-focus="autoFocus" />
                                        <p data-ng-show="planAddressForm.ElectronicPayorID.$error.maxlength" class="error-block">Maximum 20 Characters permitted.</p>
                                     </div>
                                </div>
                                <div class="col-md-3 padding-bottom-small">
                                    <div class="form-group-lg padding-bottom-small" ng-class="{ 'has-error' : planAddressForm.ContactID.$invalid && !planAddressForm.ContactID.$pristine }">
                                        <label for="contactID">Contract ID <sup>*</sup></label>
                                        <input type="text" class="form-control ellipsis" data-ng-maxlength="20" ng-model="planAddress.ContactID" data-validation-name="Contract ID" name="ContactID" ng-required="true" />
                                        <p data-ng-show="planAddressForm.ContactID.$error.maxlength" class="error-block">Maximum 20 Characters permitted.</p>
                                    </div>
                                </div>
                                <div ass="col-md-10 padding-bottom-small">
                                    @Html.Partial("../Shared/_Address")
                                </div>
                                <div class="col-md-3 form-group-lg padding-bottom-small" ng-class="{ 'has-error' : planAddressForm.effectiveDate.$invalid && !planAddressForm.effectiveDate.$pristine}">
                                    <label for="effectiveDate">Effective Date <sup>*</sup></label>
                                    <xen-date-picker id="effectiveDate" data-validation-name="Effective Date" date-required="true" data-compare-element-name="expirationDate" ng-model="planAddress.EffectiveDate" end-date="planAddress.ExpirationDate" greater-than-date-validation="true" name="effectiveDate" ignore-time="true"></xen-date-picker>
                                    <p ng-cloak ng-show="planAddressForm.effectiveDate.$error.date && !planAddressForm.effectiveDate.$pristine" class="error-block">Effective Date is invalid.</p>
                                    <p ng-show="planAddressForm.effectiveDate.$error.greaterThanDate" ng-cloak class="error-block">Effective date can't be greater than Expiration date.</p>
                                </div>
                                <div class="col-md-3 form-group-lg padding-bottom-small" ng-class="{'has-error' : planAddressForm.expirationDate.$invalid && !planAddressForm.expirationDate.$pristine}">
                                    <label for="expirationDate">Expiration Date</label>
                                    <xen-date-picker id="expirationDate" data-validation-name="Expiration Date" data-compare-element-name="effectiveDate" ng-model="planAddress.ExpirationDate" past-date-validate="true" start-date="planAddress.EffectiveDate" less-than-date-validation="true" name="expirationDate" ignore-time="true"></xen-date-picker>
                                    <p ng-show="planAddressForm.expirationDate.$error.pastDate" ng-cloak class="error-block">Expiration date can't be Past Date.</p>
                                    <p ng-cloak ng-show="planAddressForm.expirationDate.$error.date && !planAddressForm.expirationDate.$pristine" class="error-block">Expiration Date is invalid.</p>
                                    <p ng-show="planAddressForm.expirationDate.$error.lessThanDate" ng-cloak class="error-block">Expiration date can't be less than Effective date.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row padding-top-small">
                        <div class="col-lg-6 col-md-6">
                            <p class="uppercase subtext"><sup>*</sup> Indicates a required field</p>
                        </div>
                        <div class="col-lg-6 col-md-6 text-right nopadding-left">
                            <button type="submit" class="btn btn-default btn-lg no-security" ng-click="cancel()" value="Reset">CANCEL</button>
                            <xen-submit id="btnSave" name="btnSave" value="SAVE" is-next="false" mandatory="true" style="btn btn-default btn-lg" permission-key="{{permissionKey}}" permission="{{resolvePermission(pageSecurity)}}" ></xen-submit>
                        </div>
                    </div>
                </div>
            </form>
            &nbsp;
        </div>
    </div>
</div>
