@{
    ViewBag.Title = "Address";
    Layout = null;
}

efddf
<div class="container">
    <div class="row">
        <div class="col-md-12 AllCenter">
            <div class="Pageheading-section PageHeading" style="padding-left:12px;">
                <h4>Address</h4>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-12 AllCenter"><div class="HLine"></div></div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-12 AllCenter">
            <table width="70%" border="0" cellpadding="4" cellspacing="4" class="FormTable" style="min-width:700px;">
                <tr>
                    <td align="right">Address Type :</td>
                    <td>
                        <select ng-model="newDemography.Addresses.AddressType">
                            <option value="1" selected="selected">Permanent</option>
                            <option value="2">Residential</option>
                            <option value="3">Rented</option>
                        </select>
                    
                    </td>
                    <td align="right">Street1 : </td>
                    <td><input type="text" class="InputB" ng-model="newDemography.Addresses.Line1" /></td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                </tr>
                <tr>
                    <td align="right">City :</td>
                    <td><input type="text" class="InputB" ng-model="newDemography.Addresses.City" /></td>
                    <td align="right">State : </td>
                    <td>
                        <select ng-model="newDemography.Addresses.State">
                            <option value="TX" selected="selected">TX</option>
                            <option value="Alaska">Alaska</option>
                            <option value="Arizona">Arizona</option>
                            <option value="Florida">Florida</option>
                            <option value="Minnesota">Minnesota</option>
                            <option value="Ohio">Ohio</option>
                            <option value="Washington">Washington</option>
                        </select>
                    </td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                </tr>
                <tr>
                    <td align="right">Country :	</td>
                    <td>@*<input type="text" value="Tarrant" class="InputB" ng-model="County" />*@
                        <select class="InputB" ng-model="newDemography.Addresses.County">
                            <option value="Tarrant" selected="selected">Tarrant</option>
                            <option value="Henry">Henry</option>
                        </select>
                    
                    </td>
                    <td align="right">Postal Code :	</td>
                    <td><input type="text" class="InputB" ng-model="newDemography.Addresses.Zip" /></td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                </tr>
                <tr>
                    <td align="right">Apartment Complex Name :	</td>
                    <td><input type="text" value="Tarrant" class="InputB" ng-model="newDemography.Addresses.AptComplexName" /></td>
                    <td align="right">Gate Code :	</td>
                    <td><input type="text" class="InputB" ng-model="newDemography.Addresses.GateCode" /></td>
                    <td>&nbsp;</td>
                    <td>&nbsp;</td>
                </tr>
            </table>
        </div>
    </div>
</div>